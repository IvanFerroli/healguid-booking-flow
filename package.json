{
  "name": "healguid",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:init": "pnpm db:push && pnpm db:seed",
    "dev:bootstrap": "pnpm db:init && pnpm dev",
    "dev:full": "concurrently -k -n \"next,stripe,studio\" \"pnpm dev\" \"stripe listen --events checkout.session.completed --forward-to http://localhost:3000/api/webhooks/stripe\" \"pnpm db:studio\""
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^5.22.0",
    "date-fns": "^4.1.0",
    "next": "^16.0.8",
    "prisma": "^5.22.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.68.0",
    "stripe": "^20.0.0",
    "vercel": "^49.1.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20.19.25",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.1.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.0.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3"
  }
}
